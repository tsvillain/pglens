#!/usr/bin/env node

const { program } = require('commander');
const { startServer } = require('../src/server');

program
  .name('pglens')
  .description('A simple PostgreSQL database viewer tool')
  .version('1.0.0');

program
  .command('start')
  .description('Start the pglens server')
  .action(() => {
    startServer();
  });

program.parse(process.argv);

